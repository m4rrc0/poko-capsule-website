{
  "name": "poko-capsule-website",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "dependencies": {
    "@markdoc/markdoc": "^0.4.0",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@keystatic/core": "^0.5.46",
    "@keystatic/next": "^5.0.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@types/bun": "latest",
    "@types/react": "^19.0.8",
    "concurrently": "^9.1.2",
    "dayjs": "^1.11.13",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build:cms": "next build",
    "dev:cms": "next dev",
    "start:cms": "next start",
    "preview:cms": "next start",
    "dev": "concurrently \"eleventy --serve\" \"bun run dev:cms\"",
    "start": "bun run dev",
    "build": "concurrently \"eleventy\" \"bun run build:cms\"",
    "preview:11ty": "eleventy-dev-server --dir=dist",
    "preview": "concurrently \"bun run preview:11ty\" \"bun run preview:cms\""
  }
